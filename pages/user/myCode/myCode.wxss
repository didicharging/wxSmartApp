/* pages/user/myCode/myCode.wxss */

.content{
    width: 650rpx;
    margin: 0rpx 50rpx 0rpx 50rpx;
    background-color: #fff;
    overflow: hidden;
    border-radius: 16rpx;
}
.head{
   position: relative;
   padding:40rpx 40rpx;
  top:-104rpx;
  z-index:200;
  background:#fff;
  margin:0 40rpx;
  /*border:2rpx solid #ccc;*/
  box-shadow: 3px 3px 3px #888888;
}
.head .head-img{
    display: inline-block;
    width: 120rpx;
    height: 120rpx;
    vertical-align: middle;
    margin-right: 20rpx;
    /*border: 2px solid #999;*/
    border-radius: 50%;
}
.head>view{
    display: inline-block;
}
.head>view .name{
    display: inline-block;
}
.head>view image{
    display: inline-block;
    width: 40rpx;
    height: 40rpx;
    vertical-align: middle;
    margin-left: 10rpx;
}
.code image{
    width: 600rpx;
    height: 600rpx;
    margin: 25rpx 25rpx;
}
.tip{
    text-align: center;
    margin-bottom: 25rpx;
}
.head-yellow{
  width: 100%;
  height: 150rpx;
  background: #FFD843;
}
.headone{
  margin-top: 20rpx;
}